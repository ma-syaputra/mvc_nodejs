<% include ./../template/header %>
<% include ./../template/nav %>

<div class="container">
                <h3>List User</h3>
        <table class="table table-dark table-hover">
                <thead>
                        <tr>
                                <th>No</th>
                                <th>ID</th>
                          <th>Username</th>
                          <th>Name</th>
                          <th>Email</th>
                          <th>Action</th>
                        </tr>
                      </thead>
                      <% for(var i=0;  i< users.length; i++){%>
                        <tr>
                            <td><%= i+1; %></td>
                            <td><%= users[i].id %></td>
                            <td><%= users[i].username %></td>
                            <td><%= users[i].name %></td>
                            <td><%= users[i].email %></td>
                            <td><a href="user/update/<%= users[i].id %>" class="btn btn-warning">Edit</a></td>
                            

                        </tr>
                        <% } %>
                        </table>
</div>


<% include ./../template/footer %>
